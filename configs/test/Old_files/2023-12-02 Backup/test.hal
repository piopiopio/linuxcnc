#******************
#**Kinematics
#******************
loadrt [KINS]KINEMATICS
loadrt [EMCMOT]EMCMOT base_period_nsec=[EMCMOT]BASE_PERIOD servo_period_nsec=[EMCMOT]SERVO_PERIOD num_joints=[KINS]JOINTS
#loadrt hal_parport cfg="0 out 0xcc00 in"
#loadrt hal_parport cfg="0 out 0xdc00 in"
loadrt hal_parport cfg="0 out 0xcc00 in"
setp parport.0.reset-time 5000
loadrt stepgen step_type=0,0,0,0

#******************
#** Base thread
#******************
addf parport.0.read base-thread
addf parport.1.read base-thread
addf stepgen.make-pulses base-thread
addf parport.0.write base-thread
addf parport.0.reset base-thread
addf parport.1.write base-thread

#******************
#**Servo thread
#******************
addf stepgen.capture-position servo-thread
addf motion-command-handler servo-thread
addf motion-controller servo-thread
addf stepgen.update-freq servo-thread

#******************
#**Outputs to Motors
#******************
net xstep	=> parport.0.pin-02-out
net xdir	=> parport.0.pin-03-out
net y1step            => parport.0.pin-06-out
net y1dir            => parport.0.pin-07-out
net y2step           => parport.0.pin-04-out
net y2dir            => parport.0.pin-05-out
net zstep           => parport.0.pin-08-out
net zdir            => parport.0.pin-09-out


#******************
#**Estop
#******************
loadrt estop_latch
addf estop-latch.0 servo-thread
net estop-loopout iocontrol.0.emc-enable-in <= estop-latch.0.ok-out
net estop-loopin iocontrol.0.user-enable-out => estop-latch.0.ok-in
net estop-reset iocontrol.0.user-request-enable => estop-latch.0.reset
#net remote-estop estop-latch.0.fault-in <= parport.1.pin-02-in


#******************
